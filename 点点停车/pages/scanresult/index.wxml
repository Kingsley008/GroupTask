<!--pages/scanresult/index.wxml-->
<view class="container">
    <view class="password-title">
        <text>停车成功</text>
    </view>
    <view class="password-content">
        <text>车位号</text>
        <div>{{parkNumber}}</div>
    </view>
    <view class="tips">
        <text>{{time}}s后开始计费</text>
        <view class="tips-action" bindtap="moveToWarn">
            有问题?
            <text class="tips-href">回首页去车辆报障</text>
        </view>
    </view>
</view>